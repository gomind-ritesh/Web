{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "result": {
            "type": "string",
            "enum": ["success"]
        },
        "data": {
            "type": "string",
            "description": "JSON string containing orders data that must conform to #/definitions/ordersData when parsed"
        }
    },
    "required": ["result", "data"],
    "additionalProperties": false,
    "definitions": {
        "ordersData": {
            "type": "object",
            "properties": {
                "orders": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/orderItem"
                    }
                },
                "totalPages": {
                    "type": "integer",
                    "minimum": 1
                }
            },
            "required": ["orders", "totalPages"],
            "additionalProperties": false
        },
        "orderItem": {
            "type": "object",
            "properties": {
                "user_name": {
                    "type": "string"
                },
                "bill_id": {
                    "type": "integer",
                    "minimum": 1
                },
                "total_price": {
                    "type": "number",
                    "pattern": "^[0-9.]+$"
                },
                "status": {
                    "type": "string",
                    "enum": ["completed", "cancel", "active"]
                },
                "bill_date": {
                    "type": "string",
                    "format": "date"
                }
            },
            "required": [
                "user_name",
                "bill_id",
                "total_price",
                "status",
                "bill_date"
            ],
            "additionalProperties": false
        }
    }
}